                        </ui:VisualElement>
                        <ui:VisualElement name="consentFormdataData7" style="flex-grow: 0;">
                            <ui:Label tabindex="-1" text="You are free to choose whether to be in this study. If you choose to participate, it&apos;s okay to stop at any point during the session. Please pause or stop the session if you become bored or simply no longer want to participate." parse-escape-sequences="true" display-tooltip-when-elided="true" class="consentformData" style="white-space: normal;" />
                        </ui:VisualElement>
                    </ui:VisualElement>
