                        </ui:VisualElement>
                        <ui:VisualElement name="consentFormdataData8" style="flex-grow: 0;">
                            <ui:Label tabindex="-1" text="You will be entered into a raffle to receive a $10 gift card after participation in the study. The gift card code will be sent to the email address on file." parse-escape-sequences="true" display-tooltip-when-elided="true" class="consentformData" style="white-space: normal;" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement name="consentFormDataContainer9" style="flex-grow: 0; align-self: auto; justify-content: flex-start; align-items: stretch; margin-top: 0; top: 98px; flex-direction: column; display: flex;">
                        <ui:VisualElement name="consentFormdataTitle9" style="flex-grow: 0; align-items: flex-start; align-self: flex-start; flex-direction: column; margin-right: 50px;">
                            <ui:Label tabindex="-1" text="What if I have questions? " parse-escape-sequences="true" display-tooltip-when-elided="true" class="consentformDataTitle" />
                        </ui:VisualElement>
                        <ui:VisualElement name="consentFormdataData9" style="flex-grow: 0;">
                            <ui:Label tabindex="-1" text="This study is run by Dr. Nicole Cruse of Sacred Heart University. If you have any questions or concerns about this study, or in the very unlikely event of a research-related issue, please contact Dr. Cruse at crusen2@sacredheart.edu  (203) 416-3947. If you have concerns about the treatment of research participants, you can also contact the Sacred Heart University Institutional Review Board, 5151 Park Ave., Fairfield, CT 06825. The phone number is (203) 396-8454. The email address is carlj@sacredheart.edu." parse-escape-sequences="true" display-tooltip-when-elided="true" class="consentformData" style="white-space: normal;" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement name="consentFormDataContainer10" style="flex-grow: 0; align-self: auto; justify-content: flex-start; align-items: stretch; margin-top: 0; top: 98px; flex-direction: column; display: flex;">
                        <ui:VisualElement name="consentFormdataTitle10" style="flex-grow: 0; align-items: flex-start; align-self: flex-start; flex-direction: column; margin-right: 50px;">
